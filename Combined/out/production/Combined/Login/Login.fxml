<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="605.0" prefWidth="978.0" style="-fx-background-color: #0FBCF9;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Login.LoginController">
   <children>
      <HBox alignment="CENTER" maxWidth="-Infinity" scaleX="1.0" scaleY="1.0" style="-fx-background-color: #0FBCF9;">
         <children>
            <ImageView fitHeight="128.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/tooth.png" />
               </image>
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </ImageView>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: Quicksand; -fx-font-weight: bold;" text="HudDental">
               <font>
                  <Font size="36.0" />
               </font>
            </Text>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="120.0" />
         </VBox.margin>
      </HBox>
      <HBox fx:id="phoneHBox" alignment="CENTER_RIGHT" scaleX="1.0" scaleY="1.0" VBox.vgrow="NEVER">
         <children>
            <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefWidth="128.0" style="-fx-text-fill: WHITE;" text="Phone Number" textAlignment="RIGHT">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font></Label>
            <TextField fx:id="phoneField" prefWidth="192.0">
               <font>
                  <Font size="14.0" />
               </font></TextField>
         </children>
         <VBox.margin>
            <Insets bottom="40.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_RIGHT" scaleX="1.0" scaleY="1.0">
         <children>
            <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefWidth="128.0" style="-fx-text-fill: WHITE;" text="Password" textAlignment="RIGHT">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
               <font>
                  <Font size="18.0" />
               </font></Label>
            <PasswordField fx:id="passwordField" prefWidth="192.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font size="14.0" />
               </font></PasswordField>
         </children>
         <VBox.margin>
            <Insets bottom="40.0" />
         </VBox.margin>
      </HBox>
      <Button fx:id="logInButton" mnemonicParsing="false" style="-fx-border-radius: 5em; -fx-background-radius: 5em; -fx-background-color: WHITE; -fx-font-weight: bold;" text="Submit">
         <VBox.margin>
            <Insets bottom="16.0" />
         </VBox.margin></Button>
      <Label fx:id="errorLabel" style="-fx-font-weight: bold;" textFill="WHITE" />
      <ImageView fx:id="loader" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</VBox>
